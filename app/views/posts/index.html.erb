<h1>Posts</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>Overtime Request</th>
      <th>Date</th>
      <th>Rationale</th>
      <th>User</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody id="posts">
  	
  		<%= render @posts %>
   
  </tbody>
</table>

<div id="paginator">
  <%= paginate @posts, remote: true  %>
</div>